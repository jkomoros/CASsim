{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "npm run generate && npm run serve",
    "start:clean": "npm install && npm run build:clean && npm run start",
    "serve": "tsc --watch --preserveWatchOutput & wds --node-resolve --port=8081",
    "build": "npm run build:clean && npm run build:typescript && npm run build:rollup && npm run build:service-worker",
    "build:clean": "npm run build:clean:build && npm run build:clean:typescript",
    "build:clean:build": "rm -rf build/",
    "build:clean:typescript": "find src -name '*.map' -type f -delete && find src -name '*.js' -type f -delete && find src -name '*.d.ts' ! -name 'modules.d.ts' -type f -delete",
    "build:rollup": "rollup -c",
    "build:typescript": "tsc",
    "build:service-worker": "workbox generateSW workbox-config.js",
    "generate": "npm run generate:types && npm run generate:schema && npm run generate:config",
    "generate:types": "npx ts-node tools/types.ts",
    "generate:schema": "ts-json-schema-generator --type 'PackedRawSimulationConfig' --tsconfig tsconfig.json > data-schema.json",
    "generate:config": "npx ts-node tools/config.ts",
    "screenshots": "npx ts-node tools/screenshot.ts",
    "screenshots:png": "npx ts-node tools/screenshot.ts png",
    "screenshots:gif": "npx ts-node tools/screenshot.ts gif",
    "deploy": "npm run build && firebase deploy",
    "test": "npm run test:options",
    "test:options": "mocha -r esm test/options --timeout=10000"
  },
  "dependencies": {
    "@types/d3": "^7.4.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "canvas": "^2.8.0",
    "d3": "^7.3.0",
    "esm-seedrandom": "^3.0.5",
    "firebase": "^9.6.2",
    "firebase-tools": "^10.0.1",
    "lit": "^2.2.6",
    "polymer": "^0.5.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-dynamic-import-vars": "^1.4.3",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/pngjs": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-rollup": "^0.3.17",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "eslint": "^7.18.0",
    "esm": "^3.2.25",
    "gifencoder": "^2.0.1",
    "glob": "^7.2.0",
    "image-size": "^0.9.3",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.4.0",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^12.0.0",
    "rollup": "^2.75.6",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-json-schema-generator": "^1.0.0",
    "ts-lit-plugin": "^1.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wct-mocha": "^1.0.0",
    "workbox-cli": "^6.5.3"
  }
}
