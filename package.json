{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "polymer serve",
    "build": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:options",
    "test:options": "mocha -r esm test/options --timeout=10000",
    "generate": "node screenshot.js",
    "generate:screenshot": "node screenshot.js screenshot",
    "generate:gif": "node screenshot.js gif"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "canvas": "^2.8.0",
    "esm-seedrandom": "^3.0.5",
    "gulp": "^4.0.0",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "eslint": "^7.18.0",
    "esm": "^3.2.25",
    "gifencoder": "^2.0.1",
    "glob": "^7.2.0",
    "image-size": "^0.9.3",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.4.0",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^12.0.0",
    "wct-mocha": "^1.0.0"
  }
}
