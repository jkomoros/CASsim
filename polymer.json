{
	"entrypoint": "index.html",
	"shell": "src/components/my-app.js",
	"fragments": [
		"src/simulators/schelling-org.js"
	],
	"sources": [
		"images/**/*",
		"data/**/*"
	],
	"extraDependencies": [
		"manifest.json",
		"node_modules/@webcomponents/webcomponentsjs/**",
		"push-manifest.json"
	],
	"builds": [
		{
			"name": "es6-bundled",
			"browserCapabilities": [
				"es2015"
			],
			"js": {
				"compile": "es2015",
				"minify": true,
				"transformModulesToAmd": true
			},
			"css": {
				"minify": true
			},
			"html": {
				"minify": true
			},
			"bundle": true,
			"addServiceWorker": true
		}
	],
	"moduleResolution": "node",
	"npm": true
}
